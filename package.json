{
  "name": "server",
  "version": "1.0.0",
  "description": "cia platform server",
  "main": "server.js",
  "scripts": {
    "test": "nodemon test.js",
    "start": "node server.js",
    "dev": "nodemon --ignore frontend/ server.js",
    "generate_keys": "node ./routes/auth/jwt/generate_Keys.js",
    "push": "git add . && git commit -m 'changes pushed' && git push",
    "deploy": "npm run static-build && npm run irenic-build && git add . && git commit -m 'server and client commit' && git push",
    "static-build": "cd ./client/home && npm run build && cd ../../ ",
    "irenic-build": "cd ./client/irenic && npm run build && cd ../../ ",
    "setup": "npm i && cd client/home && npm i && cd .. && cd irenic && npm i && cd ../../ && node log.js setup"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "admin-bro": "^2.7.0",
    "admin-bro-expressjs": "^2.1.1",
    "admin-bro-mongoose": "^0.5.2",
    "admin-bro-theme-dark": "^1.0.0",
    "ascii-art": "^2.5.0",
    "ascii-art-image": "^1.2.2",
    "axios": "^0.19.2",
    "axios-cache-adapter": "^2.5.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.22"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
